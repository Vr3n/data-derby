{
    "compilerOptions": {
        "strict": true,
        "esModuleInterop": true,
        "jsx": "react-jsx",
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "skipLibCheck": true,
        "baseUrl": ".",
        "paths": {
            "~/*": [
                "./src/*"
            ]
        },
        // for openapi-react-query.
        "noUncheckedIndexedAccess": true
    }
}